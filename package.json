        "axios": "^1.6.0",
        "cloudinary": "^2.8.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "sharp": "^0.32.6",
        "typescript": "^5.2.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.8",
        "@types/supertest": "^2.0.16",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.9.1",
        "eslint": "^8.52.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "supertest": "^6.3.3",
